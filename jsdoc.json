

{
	"plugins": ["./node_modules/jsdoc-export-default-interop/dist/index"],
	"recurseDepth": 10,
	"source": {
		"includePattern": ".+\\.js(doc|x)?$",
    	"excludePattern": "node_modules|examples"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": true
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./docs/",
		"recurse": true,
		"tutorials": "./docs/tutorials"
	}
}

