

{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": true
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./docs/",
		"recurse": true,
		"tutorials": "./docs/examples"
	}
}

